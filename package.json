      },
      {
        "command": "bracket_quote_tools.replaceBracketsOrQuotes",
        "title": "bracket replaceSelection",
        "category": "Cursor To Search Selector"
      }
    ]
  },
  "scripts": {
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/node": "^24.3.0",
    "typescript": "^5.9.2",
    "vscode": "^1.1.37"
