{
  "name": "bracket_quote_tools",
  "displayName": "Bracket & Quote Tools",
  "description": "",
  "version": "1.0.0",
  "publisher": "your-name",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "bracket_quote_tools.changeSelection",
        "title": "bracket changeSelection",
        "category": "Cursor To Search Selector"
      },
      {
        "command": "bracket_quote_tools.surroundWithInput",
